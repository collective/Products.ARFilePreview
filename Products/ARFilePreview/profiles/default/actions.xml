<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n">
    <object name="object_buttons" meta_type="CMF Action Category">
        <object name="preview" meta_type="CMF Action" i18n:domain="plone">
            <property name="title" i18n:translate="">Generate Preview</property>
            <property name="description" i18n:translate=""></property>
            <property name="url_expr">string:$object_url/generate_preview</property>
            <property name="icon_expr"></property>
            <property name="available_expr">python:object.meta_type=='ATBlob'</property>
            <property name="permissions">
                <element value="Allow sendto"/>
            </property>
            <property name="visible">True</property>
        </object>
    </object>
</object>

